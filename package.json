{
  "name": "prismy-session-strategy-signed-cookie",
  "version": "0.0.1-0",
  "description": "Signed cookie session strategy for prismy",
  "keywords": [
    "prismy",
    "session",
    "cookie",
    "signed"
  ],
  "author": "Junyoung Choi <rokt33r.choi@gmail.com>",
  "homepage": "https://github.com/prismyland/prismy-session-strategy-signed-cookie",
  "license": "MIT",
  "main": "dist",
  "types": "dist",
  "files": [
    "dist/**/*",
    "!dist/specs/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prismyland/prismy-session-strategy-signed-cookie.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "prettier --check src/**/*.ts examples/*/src/**/*.ts",
    "format": "prettier --write src/**/*.ts examples/*/src/**/*.ts",
    "test": "ava --verbose",
    "test-ci": "nyc ava && nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "codecov",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "bugs": {
    "url": "https://github.com/prismyland/prismy-session-strategy-signed-cookie/issues"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/cookie-signature": "^1.0.2",
    "@types/got": "^9.4.4",
    "@types/node": "^12.0.2",
    "@types/test-listen": "^1.1.0",
    "@types/tough-cookie": "^2.3.5",
    "@types/uid-safe": "^2.1.2",
    "ava": "^1.4.1",
    "codecov": "^3.5.0",
    "got": "^9.6.0",
    "micro": "^9.3.5-canary.1",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "prismy": "^0.9.0",
    "prismy-cookie": "^0.3.0",
    "prismy-session": "0.0.1-3",
    "prismy-test-server": "^0.3.0",
    "rimraf": "^2.6.3",
    "tough-cookie": "^3.0.1",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "prismy": "^0.9.0",
    "prismy-session": "0.0.1-3",
    "prismy-cookie": "^0.3.0"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "specs/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "cookie-signature": "^1.1.0",
    "uid-safe": "^2.1.5"
  }
}
